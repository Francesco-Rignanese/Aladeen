@page
@{
    ViewData["Title"] = "Aladeen";
}









<!-- /.row-->
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col-sm-5">
                <h4 class="card-title mb-0">Traffic</h4>
                <div class="small text-muted">@DateTime.Now</div>
            </div>
            <!-- /.col-->
            <div class="col-sm-7 d-none d-md-block">
                <button class="btn btn-primary float-right" type="button" value="reload" onClick="document.location.reload(true)">
                    <i class="icon-refresh"></i>
                </button>
                <div class="btn-group btn-group-toggle float-right mr-3" data-toggle="buttons">
                    <label class="btn btn-outline-secondary">
                        <input id="option1" type="radio" name="options" autocomplete="off"> Hour
                    </label>
                   
                </div>
            </div>
            <!-- /.col-->
        </div>
        <!-- /.row-->
        <div class="chart-wrapper" style="height:300px;margin-top:40px;">
            <ul id="messagesList"></ul>
            <canvas class="chart" id="main-chart" height="300"></canvas>
        </div>
    </div>
    <div class="card-footer">
        <div class="row text-center">
            <div class="col-sm-12 col-md mb-sm-2 mb-0">
                <div class="text-muted">Auto</div>
                <strong>29.703</strong>
                <div class="progress progress-xs mt-2">
                    <div class="progress-bar bg-success" role="progressbar" style="width: 40%" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
            <div class="col-sm-12 col-md mb-sm-2 mb-0">
                <div class="text-muted">Camion</div>
                <strong>18.523</strong>
                <div class="progress progress-xs mt-2">
                    <div class="progress-bar bg-info" role="progressbar" style="width: 20%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>

            <div class="col-sm-12 col-md mb-sm-2 mb-0">
                <div class="text-muted">Moto</div>
                <strong>10.375</strong>
                <div class="progress progress-xs mt-2">
                    <div class="progress-bar bg-red" role="progressbar" style="width: 20%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>


        </div>
    </div>
</div>


